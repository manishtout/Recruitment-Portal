<h3>Reports</h3>

<table>
  <thead>
    <tr>
      <th>Interview_number</th>
      <th>Status</th>
      <th>Interviewer_name</th>
      <th>Feedback</th>
      <th>candidate_id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |can| %>
      <tr>
        <td><%= can.interview_number %></td>
        <td><%= can.status %></td>     
        <td><%= can.interviewer_name %></td> 
        <td><%= can.feedback %></td>  
        <td><%= can.candidate_id %></td>                
      </tr>
    <% end %>
  </tbody>
</table>



